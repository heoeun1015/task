<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="500.0" prefWidth="626.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kr.or.ddit.basic.H_컨트롤객체연습Controller">
   <children>
      <VBox prefHeight="420.0" prefWidth="626.0">
         <children>
            <HBox spacing="10.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="이름 : " textAlignment="CENTER">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Text>
                  <TextField fx:id="txtField" prefHeight="20.0" prefWidth="169.0">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </TextField>
               </children>
            </HBox>
            <HBox spacing="10.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="성 별 : " textAlignment="CENTER">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Text>
                  <RadioButton fx:id="gender1" mnemonicParsing="false" selected="true" text="남">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <toggleGroup>
                        <ToggleGroup fx:id="group" />
                     </toggleGroup>
                  </RadioButton>
                  <RadioButton fx:id="gender2" mnemonicParsing="false" text="여" toggleGroup="$group">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </RadioButton>
               </children>
               <padding>
                  <Insets top="15.0" />
               </padding>
            </HBox>
            <HBox prefHeight="24.0" prefWidth="500.0" spacing="10.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="취 미 : ">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Text>
                  <CheckBox fx:id="h1" mnemonicParsing="false" text="여행">
                     <font>
                        <Font size="14.0" />
                     </font></CheckBox>
                  <CheckBox fx:id="h2" mnemonicParsing="false" text="등산">
                     <font>
                        <Font size="14.0" />
                     </font></CheckBox>
                  <CheckBox fx:id="h3" mnemonicParsing="false" text="독서">
                     <font>
                        <Font size="14.0" />
                     </font></CheckBox>
                  <CheckBox fx:id="h4" mnemonicParsing="false" text="바둑">
                     <font>
                        <Font size="14.0" />
                     </font></CheckBox>
                  <CheckBox fx:id="h5" mnemonicParsing="false" text="장기">
                     <font>
                        <Font size="14.0" />
                     </font></CheckBox>
                  <CheckBox fx:id="h6" mnemonicParsing="false" text="게임">
                     <font>
                        <Font size="14.0" />
                     </font></CheckBox>
                  <CheckBox fx:id="h7" mnemonicParsing="false" text="테니스">
                     <font>
                        <Font size="14.0" />
                     </font></CheckBox>
                  <CheckBox fx:id="h8" mnemonicParsing="false" text="배드민턴">
                     <font>
                        <Font size="14.0" />
                     </font></CheckBox>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets top="15.0" />
               </padding>
            </HBox>
            <Button fx:id="btn" mnemonicParsing="false" onAction="#btnClick" prefHeight="15.0" prefWidth="65.0" text="보  기">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <font>
                  <Font size="13.0" />
               </font>
            </Button>
            <TextArea fx:id="txtResult" prefHeight="250.0" prefWidth="352.0">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </TextArea>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</Pane>
